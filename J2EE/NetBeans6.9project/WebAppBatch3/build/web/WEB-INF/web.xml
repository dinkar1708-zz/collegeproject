<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <context-param>
        <param-name>copyright</param-name>
        <param-value>www.sunmicrosystems.com</param-value>
    </context-param>
    <filter>
        <filter-name>MyFilter</filter-name>
        <filter-class>filter.MyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>MyFilter</filter-name>
        <servlet-name>SendCookie</servlet-name>
    </filter-mapping>
    <listener>
        <listener-class>listener.MyListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>xyz</servlet-name>
        <servlet-class>pack.FirstServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SecondServlet</servlet-name>
        <servlet-class>pack.SecondServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyHttpServlet</servlet-name>
        <servlet-class>pack.MyHttpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReqHandler</servlet-name>
        <servlet-class>pack.ReqHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Authentication</servlet-name>
        <servlet-class>pack.Authentication</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminControlPanel</servlet-name>
        <servlet-class>pack.AdminControlPanel</servlet-class>
        <init-param>
            <param-name>contact</param-name>
            <param-value>0987727211</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>pack.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendCookie</servlet-name>
        <servlet-class>cookietest.SendCookie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetCookie</servlet-name>
        <servlet-class>cookietest.GetCookie</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>xyz</servlet-name>
        <url-pattern>/abc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SecondServlet</servlet-name>
        <url-pattern>/SecondServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyHttpServlet</servlet-name>
        <url-pattern>/callHS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReqHandler</servlet-name>
        <url-pattern>/rh</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Authentication</servlet-name>
        <url-pattern>/auth</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminControlPanel</servlet-name>
        <url-pattern>/cp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>LoginPage.html</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>SendCookie</servlet-name>
        <url-pattern>/sc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetCookie</servlet-name>
        <url-pattern>/gc</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>1</session-timeout>
    </session-config>
</web-app>
